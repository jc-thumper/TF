<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <!-- Retry register SI service Scheduler -->
        <record id="register_service_scheduler" model="ir.cron">
            <field name="name">Retry: Register SI service</field>
            <field name="model_id" ref="model_forecasting_config_settings"/>
            <field name="active" eval="True"/>
            <field name="state">code</field>
            <field name="code">model._cron_retry_register_si_service()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">5</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
