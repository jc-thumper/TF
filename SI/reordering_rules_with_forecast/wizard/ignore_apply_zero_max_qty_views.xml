<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_ignore_apply_zero_max_qty_form" model="ir.ui.view">
        <field name="name">Ignore apply zero to max qty</field>
        <field name="model">ignore.apply.zero.new.max.qty</field>
        <field name="arch" type="xml">
            <form string="Confirmation Form">
                <center>
                    <field name="message" readonly="1"/>
                </center>
                <footer>
                    <button name="accept_confirmation" type="object" string="YES" class="oe_highlight"/>
                    <button name="cancel_confirmation" type="object" string="NO"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
