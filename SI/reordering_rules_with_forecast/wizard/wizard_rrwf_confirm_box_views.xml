<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_forecasting_confirm_box_form" model="ir.ui.view">
        <field name="name">Apply</field>
        <field name="model">wizard.rrwf.confirm.box</field>
        <field name="arch" type="xml">
            <form string="Confirmation Form">
                <field name="message" readonly="1"/>
                <field name="auto_generate_new_rr"/>
                <label for="auto_generate_new_rr"/>
                <footer>
                    <button name="accept_confirmation" type="object" string="YES" class="oe_highlight"/>
                    <button name="cancel_confirmation" type="object" string="NO"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
